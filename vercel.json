{
  "cleanUrls": true,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/server.js" },
    { "source": "/(index|newsarchive|create-post|post)", "destination": "/server.js" },
    { "source": "/", "destination": "/server.js" },
    { "source": "/(.*)", "destination": "/server.js" }
  ]
}
